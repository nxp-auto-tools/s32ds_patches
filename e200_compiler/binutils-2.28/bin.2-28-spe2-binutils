+	mftlb1cfg   1
+	mfl1finv1   1
+	mfhid0      1
+	mfhid1      1
+	mfl1csr0    1
+	mfl1csr1    1
+	mfmmucsr0   1
+	mfbucsr     1
+	mfmmucfg    1
+	mfl1finv0   1
+	mfsvr       1
+
+/* Move to SPR */
+	mtdec       1
+	mttbl       1
+	mttbu       1
+	mtpir       1
+	mtivor35    1
+	mtctxcr     1
+	mtdbcr3     1
