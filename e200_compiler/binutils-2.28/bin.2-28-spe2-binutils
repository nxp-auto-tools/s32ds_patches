+	mfdecar     1
+	mfl1cfg0    1
+	mfl1cfg1    1
+	mfivor35    1
+	mfctxcr     1
+	mfdbcr3     1
+	mfdbcnt     1
+	mfdbcr4     1
+	mfdbcr5     1
+	mfiac5      1
+	mfiac6      1
+	mfiac7      1
+	mfiac8      1
+	mfdberc0    1
+	mfmcsrr0    1
+	mfmcsrr1    1
+	mfddam      1
+	mfdbcr6     1
+	mfsprg8     1
+	mfsprg9     1
+	mfpid1      1
+	mfpid2      1
