+ * print -- do a raw print of a string
+ */ 
+void
+print (ptr)
+     char *ptr;
+{
+  int len = 0;
+  char *p = ptr;
+
+  while (*p != '\0')
+    p++;
+
+  write (1, ptr, p-ptr);
